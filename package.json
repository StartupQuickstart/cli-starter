{
  "name": "example-cli",
  "version": "0.0.1",
  "description": "This is an cli template built of off Commander.js.",
  "main": "index.js",
  "scripts": {
    "build": "npx babel src --out-dir dist && sed -i '' \"s/babel-node --/node/\" dist/cli.js",
    "postinstall": "npm run build",
    "clean": "rm -rf node_modules dist"
  },
  "author": "Thomas Boles <thomas@startupquickstart.com> (https://github.com/startupquickstart)",
  "license": "MIT",
  "bin": {
    "cli": "./dist/cli.js",
    "cli-dev": "./src/cli.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.11.0",
    "eslint-config-babel": "^9.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "prompts": "^2.4.2",
    "shelljs": "^0.8.5"
  }
}
